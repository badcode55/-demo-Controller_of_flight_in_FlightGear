<?xml version="1.0" encoding="UTF-8"?>
<PropertyList>
    <logging>
        <log>
            <!--filename 新版本的fg 应该为绝对路径-->
            <!--
            将该文件转移到C:\Users\lenovo\AppData\Roaming\flightgear.org
            命令行输入如下，使用绝对路径：
            config前面两个“-’，由于xml备注的格式问题，就只写了一个，记得手动补上
            fgfs -config="C:\Users\lenovo\AppData\Roaming\flightgear.org\log-config.xml"

        -->
            <filename>C:\Users\huangbingjie\AppData\Roaming\flightgear.org\Export\steering.csv</filename>
            <enabled>true</enabled>
            <interval-ms>1000</interval-ms>
            <!--
       interval-ms 是定义采集数据的时间间隔，单位毫秒
   -->
            <delimiter>,</delimiter>
            <!--例子：
   <entry>
    <enabled>true</enabled>
    <title>Rudder</title>
    <property>/controls/rudder</property>
   </entry>
   -->
            <entry>
                <enabled>true</enabled>
                <title>aileron</title>
                <property>/controls/flight/aileron</property>
            </entry>

            <entry>
                <enabled>true</enabled>
                <title>elevator</title>
                <property>/controls/flight/elevator</property>
            </entry>
            <entry>
